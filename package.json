  },
  "homepage": "https://github.com/alfovo/mysterious-comments#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "dotenv": "^8.2.0",
    "mysql": "^2.18.1"
  }
